   RowBox[{
    RowBox[{"\"\<4-Velocity\>\"", "[", "\"\<\[Mu]\>\"", "]"}], ".", 
    RowBox[{"\"\<4-Velocity\>\"", "[", "\"\<\[Mu]\>\"", "]"}]}], 
