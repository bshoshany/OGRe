 RowBox[{
  RowBox[{"TDelete", "[", "\"\<ParallelizationTestChristoffel\>\"", "]"}], 
  ";"}]], "Input",
 CellLabel->
  "In[213]:=",ExpressionUUID->"01c9cbea-10c4-4e35-8ead-cdea25ec967a"],

